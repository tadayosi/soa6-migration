<?xml version="1.0" encoding="UTF-8"?>
<route xmlns="http://camel.apache.org/schema/spring" id="org.switchyard.quickstarts.helloworld.SimpleListener"
 errorHandlerRef="loggingErrorHandler">
    <bean beanType="org.switchyard.quickstarts.helloworldaction.MyJMSListenerAction" method="displayMessage" />
    <bean beanType="org.switchyard.quickstarts.helloworldaction.MyJMSListenerAction" method="playWithMessage"/>
    <log message="[message] '${body}'" />
    <to uri="switchyard://notificationAction" />
</route>
